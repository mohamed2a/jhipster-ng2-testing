    "enableSocialSignIn": false,
    "useSass": false,
    "applicationType": "monolith",
    "angularVersion": "angular2",
    "testFrameworks": [
      "protractor"
    ],
    "jhiPrefix": "jhi",
    "enableTranslation": true,
    "nativeLanguage": "en",
    "languages": [
      "en"
    ]
